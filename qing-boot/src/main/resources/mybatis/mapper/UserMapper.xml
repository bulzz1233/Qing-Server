<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qing.qingboot.mapper.UserMapper">

    <update id="updateId">
        update user set sex = #{sex} , tall = #{tall},weight=#{weight} where uid = #{uid}
    </update>
</mapper>